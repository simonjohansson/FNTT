/**
 *  Exclusive-or gate: out = !(a == b).
 */

CHIP Xor {

    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=firstNand);
    Nand(a=a, b=firstNand, out=secondNand);
    Nand(a=firstNand, b=b, out=thirdNand);
    Nand(a=secondNand, b=thirdNand, out=out);
}
